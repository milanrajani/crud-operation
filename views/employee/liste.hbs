<h3><a class="btn btn-secondary" href="/employee"><i class="fa fa-plus"></i> Create New</a> Task List</h3>
<a class="btn btn-secondary" href="/schedule"><i class="fa fa-time"></i>Schedule Task</a>
{{!-- <h3><a class="btn btn-secondary" href="/employee"><i class="fa fa-plus"></i> Create New</a</h3> --}}
<table class="table table-striped">
    <thead>
        <tr>
            <th>Task</th>
            <th>Title</th>  
            <th>Date</th>
            <th>Created At</th>
        </tr>
    </thead>
    <tbody>
        {{#each list}}
        <tr>
            <td>{{this.task}}</td>
            <td>{{this.title}}</td>
            <td>{{this.date}}</td>
            <td>{{this.created_at}}</td>
            <td>
                <a href="/schedule/{{this._id}}"><i class="fa fa-pencil fa-lg" aria-hidden="true"></i></a>
                <a href="/schedule/delete/{{this._id}}" onclick="return confirm('Are you sure to delete this record ?');"><i class="fa fa-trash fa-lg" aria-hidden="true"></i></a>
            </td>
        </tr>
        {{/each}}
    </tbody>
</table>